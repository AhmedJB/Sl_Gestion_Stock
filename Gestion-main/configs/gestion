server {
    listen 80;
    server_name 127.0.0.1 85.31.236.214;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        alias /usr/src/app/frontend/static/$1;

    }
    location /media/ {
        alias /usr/src/app/media/$1;

    }

    location / {
        include proxy_params;
        proxy_pass http://127.0.0.1:8000;
    }
}
